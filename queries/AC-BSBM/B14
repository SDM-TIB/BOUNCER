PREFIX bsbm: <http://www4.wiwiss.fu-berlin.de/bizer/bsbm/v01/vocabulary/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>
PREFIX rev: <http://purl.org/stuff/rev#>
PREFIX bsbm-inst: <http://www4.wiwiss.fu-berlin.de/bizer/bsbm/v01/instances/>
PREFIX dc: <http://purl.org/dc/elements/1.1/>

SELECT DISTINCT ?product ?productTypeComment
WHERE {
   <http://www4.wiwiss.fu-berlin.de/bizer/bsbm/v01/instances/dataFromProducer1/Product101> rdf:type ?productType .
   <http://www4.wiwiss.fu-berlin.de/bizer/bsbm/v01/instances/dataFromProducer1/Product101> rdfs:comment ?productComment .
   <http://www4.wiwiss.fu-berlin.de/bizer/bsbm/v01/instances/dataFromProducer1/Product101> bsbm:producer ?producer .

   ?productType rdfs:comment ?productTypeComment .
   ?productType rdfs:label ?productTypeLabel .

   ?producer rdfs:label ?producerLabel .
   ?producer rdfs:comment ?producerComment.
   ?producer dc:publisher ?producerPublisher.
}